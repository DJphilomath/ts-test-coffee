{
  "name": "tstests",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test TEST_TYPE=integration jest --runInBand --forceExit"
  },
  "jest": {
    "runner": "jest-runner-tsc",
    "globals": {
      "ts-jest": {
        "tsConfig": false
      }
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "globalSetup": "<rootDir>/tests/setup.js",
    "globalTeardown": "<rootDir>/tests/teardown.js",
    "transform": {
      ".coffee$": "<rootDir>/node_modules/jest-coffee-preprocessor/index.js",
      ".(ts|tsx)$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/services/*.ts",
      "src/routes/*.ts"
    ],
    "moduleFileExtensions": [
      "json",
      "ts",
      "tsx",
      "js",
      "jsx",
      "coffee",
      "node"
    ],
    "transformIgnorePatterns": [],
    "cache": false
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "4.16.1",
    "@types/jest": "^24.0.11",
    "@types/supertest": "2.0.7",
    "coffee-register": "1",
    "coffee-script": "1.12.7",
    "express": "4.16.4",
    "jest": "^24.5.0",
    "jest-coffee-preprocessor": "^1.0.0",
    "jest-runner-tsc": "~1.3.0",
    "source-map-support": "^0.5.11",
    "supertest": "4.0.0",
    "ts-jest": "^24.0.0",
    "typescript": "3.3.3"
  }
}
